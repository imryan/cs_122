package css_122_Override;

/*
 * Example of overloading
 */

public class Overload {

	public static void main(String[] args) {
		double a = 2, b = 2;
		sum(a, b);

		int x = 4, y = 4;
		sum(x, y);
	}

	public static int sum(int a, int b) {
		return a + b;
	}

	public static double sum(double a, double b) {
		return a + b;
	}
}
